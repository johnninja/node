<%- include header%>
<form method="post">
	<div class="form-group">
		<label for="">标题</label>
		<input class="form-control" type="text" name="title" value="<%= post.title%>">
	</div>
	<div class="form-group">
		<label for="">正文</label>
		<textarea name="post" id="" cols="30" rows="10">
			<%= post.post%>
		</textarea>
	</div>
	<input type="submit" value="保存修改">
</form>
<%- include footer%>
